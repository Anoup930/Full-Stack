#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules/.pnpm/cli-highlight@2.1.11/node_modules/cli-highlight/bin/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules/.pnpm/cli-highlight@2.1.11/node_modules/cli-highlight/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules/.pnpm/cli-highlight@2.1.11/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules/.pnpm/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/node_modules:/mnt/c/Users/aualiqul/Desktop/node_modules:/mnt/c/Users/aualiqul/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules/.pnpm/@vue+cli-service@5.0.4/node_modules/@vue/cli-service/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules/.pnpm/@vue+cli-service@5.0.4/node_modules/@vue/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules/.pnpm/@vue+cli-service@5.0.4/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules/.pnpm/cli-highlight@2.1.11/node_modules/cli-highlight/bin/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules/.pnpm/cli-highlight@2.1.11/node_modules/cli-highlight/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules/.pnpm/cli-highlight@2.1.11/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules/.pnpm/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/node_modules:/mnt/c/Users/aualiqul/Desktop/node_modules:/mnt/c/Users/aualiqul/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules/.pnpm/@vue+cli-service@5.0.4/node_modules/@vue/cli-service/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules/.pnpm/@vue+cli-service@5.0.4/node_modules/@vue/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules/.pnpm/@vue+cli-service@5.0.4/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../cli-highlight@2.1.11/node_modules/cli-highlight/bin/highlight" "$@"
else
  exec node  "$basedir/../../../../../../cli-highlight@2.1.11/node_modules/cli-highlight/bin/highlight" "$@"
fi
