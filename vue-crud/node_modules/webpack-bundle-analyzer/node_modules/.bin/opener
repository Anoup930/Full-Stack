#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules/.pnpm/opener@1.5.2/node_modules/opener/bin/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules/.pnpm/opener@1.5.2/node_modules/opener/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules/.pnpm/opener@1.5.2/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules/.pnpm/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/node_modules:/mnt/c/Users/aualiqul/Desktop/node_modules:/mnt/c/Users/aualiqul/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules/.pnpm/webpack-bundle-analyzer@4.5.0/node_modules/webpack-bundle-analyzer/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules/.pnpm/webpack-bundle-analyzer@4.5.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules/.pnpm/opener@1.5.2/node_modules/opener/bin/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules/.pnpm/opener@1.5.2/node_modules/opener/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules/.pnpm/opener@1.5.2/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules/.pnpm/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/node_modules:/mnt/c/Users/aualiqul/Desktop/node_modules:/mnt/c/Users/aualiqul/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules/.pnpm/webpack-bundle-analyzer@4.5.0/node_modules/webpack-bundle-analyzer/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules/.pnpm/webpack-bundle-analyzer@4.5.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../opener@1.5.2/node_modules/opener/bin/opener-bin.js" "$@"
else
  exec node  "$basedir/../../../../../opener@1.5.2/node_modules/opener/bin/opener-bin.js" "$@"
fi
