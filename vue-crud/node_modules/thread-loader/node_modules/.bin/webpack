#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules/.pnpm/webpack@5.71.0/node_modules/webpack/bin/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules/.pnpm/webpack@5.71.0/node_modules/webpack/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules/.pnpm/webpack@5.71.0/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules/.pnpm/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/node_modules:/mnt/c/Users/aualiqul/Desktop/node_modules:/mnt/c/Users/aualiqul/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules/.pnpm/thread-loader@3.0.4_webpack@5.71.0/node_modules/thread-loader/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules/.pnpm/thread-loader@3.0.4_webpack@5.71.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules/.pnpm/webpack@5.71.0/node_modules/webpack/bin/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules/.pnpm/webpack@5.71.0/node_modules/webpack/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules/.pnpm/webpack@5.71.0/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules/.pnpm/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/node_modules:/mnt/c/Users/aualiqul/Desktop/node_modules:/mnt/c/Users/aualiqul/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules/.pnpm/thread-loader@3.0.4_webpack@5.71.0/node_modules/thread-loader/node_modules:/mnt/c/Users/aualiqul/Desktop/Learning/vue-crud/node_modules/.pnpm/thread-loader@3.0.4_webpack@5.71.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../webpack@5.71.0/node_modules/webpack/bin/webpack.js" "$@"
else
  exec node  "$basedir/../../../../../webpack@5.71.0/node_modules/webpack/bin/webpack.js" "$@"
fi
